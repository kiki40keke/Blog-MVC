{
  "require": {
    "php": ">=8.2",
    "altorouter/altorouter": "^2.0",
    "symfony/var-dumper": "^7.3",
    "filp/whoops": "^2.18",
    "fakerphp/faker": "^1.24",
    "vlucas/valitron": "^1.4",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App/"
    }
  },
  "autoload-dev": {
  "psr-4": {
    "Tests\\": "tests/"
  }
},
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  },
"scripts": {
  "test": "phpunit --colors=always",
  "unit": "phpunit --colors=always --testsuite=unit",
  "helpers": "phpunit --colors=always --testsuite=helpers",
  "models": "phpunit --colors=always --testsuite=models",
  "validators": "phpunit --colors=always --testsuite=validators"
}


}
